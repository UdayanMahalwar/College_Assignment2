#include<iostream>
using namespace std;
bool linear(int arr[],int size,int target)
{
    for(int i=0;i<size;i++)
    {
        if(arr[i]==target)
        {
            return true;
        }
    }
    return false;
}
bool bs(int arr[], int size,int target)
{
    int s=0,e=size-1;
    while(s<e)
    {
        int mid=s+(e-s)/2;
        if(arr[mid]==target)
        {
            return true;
        }
        else if(arr[mid]>target)
        {
            e=mid-1;
        }
        else
        {
            s=mid+1;
        }
    }
    return false;
}
int main()
{
    int arr[10]={1,3,5,8,9,23,44,52,61,77};
    int target=23;
    bool ans;
    //linear search
    
    ans=linear(arr,10,target);
    cout<<ans<<endl;
    --------------------------------
    //binary search
    
    ans=bs(arr, 10,target);
    cout<<ans;
    return 0;
}